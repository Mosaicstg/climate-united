{
  "name": "climate-united",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "build:profile": "remix vite:build --profile",
    "dev": "remix vite:dev",
    "dev:debug": "node --inspect-brk node_modules/.bin/remix vite:dev",
    "start": "netlify serve",
    "typecheck": "tsc -b",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "redirects:disable": "shx rm -f public/_redirects",
    "eslint:check": "eslint --ext .js,.jsx,.ts,.tsx . --fix-dry-run",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "format": "prettier --write \"**/*.{js,jsx,tsx,ts}\"",
    "prettier:check": "prettier --check \"**/*.{js,jsx,tsx,ts}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@conform-to/react": "^1.0.1",
    "@conform-to/zod": "^1.0.1",
    "@contentful/rich-text-react-renderer": "^15.19.0",
    "@contentful/rich-text-types": "^16.3.0",
    "@nasa-gcn/remix-seo": "^2.0.0",
    "@netlify/functions": "^2.5.1",
    "@netlify/remix-adapter": "^2.2.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@remix-run/node": "^2.7.2",
    "@remix-run/react": "^2.7.2",
    "@remix-run/serve": "^2.7.2",
    "@u-wave/react-vimeo": "^0.9.10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "framer-motion": "^11.0.20",
    "isbot": "^4.1.0",
    "lucide-react": "^0.363.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.5.0",
    "shx": "^0.3.4",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@remix-run/dev": "^2.7.2",
    "@remix-run/eslint-config": "^2.7.2",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "remix-development-tools": "^4.1.4",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.2.6",
    "vite-env-only": "^3.0.3",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
